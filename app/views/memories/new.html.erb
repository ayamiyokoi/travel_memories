<div class="container">
  <div class="row">
    <div>
      <h1>New Memory</h1>
         <%= form_with model:@memory, local:true do |f| %>
         <h4>タイトル</h4>
         <%= f.text_field :title %>
         <h4>カテゴリー</h4>
         <%= f.text_field :category %>
         <h4>本文</h4>
         <%= f.text_area :body %>
         <h4>画像</h4>
         <%= f.attachment_field :image %>
         <%= f.submit '投稿' %>
         <% end %>
    </div>
  </div>
</div>